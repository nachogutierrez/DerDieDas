{
  "hosting": {
    "public": "frontend",
    "headers": [
      {
        "source": "/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=21600"
          }
        ]
      }
    ],
    "ignore": [
      "firebase.json",
      "config.example.json",
      "package.json",
      "package-lock.json",
      "**/.*",
      "**/*.test.js",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
